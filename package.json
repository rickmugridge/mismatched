{
  "name": "mismatched",
  "version": "2.9.0",
  "description": "A composable matching/assertion/validation framework in Typescript that displays mismatches as diff trees",
  "author": {
    "name": "Rick Mugridge",
    "email": "rick.mugridge@hypr.co.nz"
  },
  "keywords": [
    "matcher",
    "assertion",
    "diff"
  ],
  "license": "Apache 2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rickmugridge/expected"
  },
  "homepage": "https://github.com/rickmugridge/expected",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "engines": {
    "node": ">=10.13.0"
  },
  "scripts": {
    "clean": "rimraf dist/**",
    "compile": "tsc -p .",
    "test": "npm run clean && npm run compile && npm run retest:micro",
    "retest:micro": "mocha --exit \"./dist/src/**/*.micro.js\"",
    "publishIt": "npm login && npm publish"
  },
  "devDependencies": {
    "@types/mocha": "^9.0.0",
    "@types/node": "~16.11.6",
    "mocha": "^9.1.3",
    "rimraf": "^3.0.2",
    "ts-node": "^10.4.0",
    "prelude-ts": "1.0.3",
    "typescript": "4.2.2"
  },
  "dependencies": {
    "fast-array-diff": "^0.2.0"
  }
}
